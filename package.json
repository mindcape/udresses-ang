{
  "name": "resources",
  "version": "1.0.0",
  "description": "External dependencies",
  "main": "index.html",
  "scripts": {
    "start": "node server.js",
    "info": "echo 'npm as a build tool'",
    "lint": "echo '=> linting' && jshint js/*.js",
    "minify:css": "echo '=> minify:css' && cleancss css/app.css -o dist/public/css/app.min.css",
    "minify:js": "echo '=> minify:js' && uglifyjs js/app.js -o dist/public/js/app.min.js",
    "build:dev": "gulp",
    "build:clean": "gulp clean-dist && gulp clean-lib",
    "build:prod": "gulp & gulp copy-lib && gulp build --production && gulp updateBuildVersion"
  },
  "author": "Mahendra",
  "license": "ISC",
  "devDependencies": {
    "clean-css": "^3.4.19",
    "connect": "^3.1.x",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "jshint": "^2.9.2",
    "minimatch": "^3.0.3",
    "serve-static": "^1.4.x",
    "uglify-js": "^2.7.3"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-flexslider": "^1.3.3",
    "angular-resource": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "bootstrap": "^3.3.7",
    "components-font-awesome": "^4.6.1",
    "jquery": "^3.1.0",
    "ui-navbar": "^0.4.3"
  }
}
